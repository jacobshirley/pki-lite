{
    "name": "pki-lite-crypto-extended",
    "version": "1.0.6",
    "description": "Extended cryptographic provider for pki-lite with MD5 hashing and AES ECB/CBC support",
    "main": "dist/index.js",
    "scripts": {
        "compile": "pnpm compile:validate && tsc -p tsconfig.prod.json",
        "compile:validate": "tsc -p tsconfig.json --noEmit",
        "watch": "tsc -p tsconfig.prod.json --watch",
        "test": "vitest run",
        "test:unit": "vitest run test/unit",
        "test:acceptance": "vitest run test/acceptance --browser.enabled=false",
        "test:watch": "vitest"
    },
    "files": [
        "dist",
        "!**/*.tsbuildinfo"
    ],
    "repository": {
        "url": "https://github.com/jacobshirley/pki-lite"
    },
    "keywords": [
        "cryptography",
        "pki",
        "md5",
        "aes",
        "ecb",
        "cbc",
        "encryption",
        "hashing",
        "crypto",
        "security",
        "pkcs",
        "x509",
        "certificate",
        "webcrypto",
        "nodejs",
        "typescript",
        "pem"
    ],
    "author": "Jacob Shirley",
    "homepage": "https://jacobshirley.github.io/pki-lite/v1",
    "license": "MIT",
    "packageManager": "pnpm@10.17.1",
    "dependencies": {
        "@noble/ciphers": "2.0.1",
        "@noble/hashes": "2.0.1",
        "node-forge": "^1.3.1"
    },
    "peerDependencies": {
        "pki-lite": "^1.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.5.1",
        "@types/node-forge": "^1.3.14",
        "@vitest/browser": "^3.2.4",
        "playwright": "^1.55.0",
        "typescript": "5.9.3",
        "vitest": "^3.2.4"
    }
}
